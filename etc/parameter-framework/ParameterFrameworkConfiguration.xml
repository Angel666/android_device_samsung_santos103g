<?xml version="1.0" encoding="UTF-8"?>
<ParameterFrameworkConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Schemas/ParameterFrameworkConfiguration.xsd"
	SystemClassName="Audio" ServerPort="5000" TuningAllowed="false">
    <SubsystemPlugins>
        <Location Folder="/system/lib/parameter-framework-plugins/Audio">
            <Plugin Name="liblpe-subsystem.so"/>
            <Plugin Name="libamixer-subsystem.so"/>
            <Plugin Name="libalsactl-subsystem.so"/>
            <Plugin Name="libimc-subsystem.so"/>
            <Plugin Name="libaudience-subsystem.so"/>
            <Plugin Name="libbluetooth-subsystem.so"/>
        </Location>
        <Location Folder="/system/lib/parameter-framework-plugins/Fs">
            <Plugin Name="libfs-subsystem.so"/>
        </Location>
    </SubsystemPlugins>
    <StructureDescriptionFileLocation Path="Structure/Audio/AudioClass.xml"/>
    <SettingsConfiguration>
        <ConfigurableDomainsFileLocation Path="Settings/Audio/AudioConfigurableDomains.xml"/>
        <!--ConfigurableDomainsFileLocation Path="Settings/Audio/AudioSettingsExport.xml"/-->
        <!--BinarySettingsFileLocation Path="Settings/Audio/AudioSettingsExport.bin"/-->
    </SettingsConfiguration>
</ParameterFrameworkConfiguration>
